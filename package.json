{
	"name": "stormflash",
	"description": "stormflash provides remote application lifecycle management on any arbitrary endpoint",
	"version": "0.7.1",
	"author": "Peter K. Lee",
	"homepage": "http://stormstack.org",
	"repository": {
		"type": "git",
		"url": "git://github.com/stormstack/stormflash.git"
	},
	"dependencies": {
		"stormbolt": "0.1.x",
       	"zappajs": "0.3.x",
		"dirty": "0.9.x",
		"request": "2.9.x",
		"json-schema": "0.2.0",
		"lazy": "1.0.x",
		"async": "0.1.x",
       	"fileops":"0.1.3",
		"node-uuid": "1.3.x",
		"optimist":"0.6.0",
		"mux-demux":"3.7.8",
		"find-in-path":"0.0.x",
		"forever-monitor":"1.2.x",
		"openssl-wrapper":"0.2.x",
		"events":"1.0.x"
	},
	"devDependencies": {
		"coffee-script": ">=1.3"
	},
	"optionalDependencies": {},
	"engines": {
		"node": ">=0.6.x"
	},
	"main": "./lib/stormflash",
	"config": {
		"port": 5000,
		"logfile": "/var/log/stormflash.log",
		"datadir": "/var/stormflash",
		"stormstack": {
			"enabled": true,  // set to false for manual configuration
			"tracker": "", // specify explicit stormtracker URL
			"bolt": "/etc/stormstack/stormbolt.json",
			"autobolt": true
		}
	},
	"scripts": {
		"prepublish": "find src -name '*.coffee' | xargs coffee -o lib -c",
		"start": "node lib/app.js"
	},
	"contributors": [
		"Suresh",
		"Geetha",
		"Ravi",
		"Siva"
	]
}
